{
  "version": 2,
  "buildCommand": "npm install && npm run build",
  "routes": [
    {
      "src": "/api/.*",
      "dest": "api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/build/$1"
    }
  ],
  "functions": {
    "api/index.py": {
      "maxDuration": 60,
      "includeFiles": "api/utils/**",
      "excludeFiles": "**/__pycache__/**"
    }
  }
}